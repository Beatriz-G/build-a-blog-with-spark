{{#partial "content"}}
    <section>
        <div class ="container">
            <div class = "entry-list">
                {{#each entries}}
                    <article>
                        <h2><a href="/entries/{{slug}}">{{title}}</a></h2>
                        <time datetime={{date}}>{{formattedDate}}</time>
                    </article>
                {{/each}}
            </div>
        </div>
    </section>
{{/partial}}

{{> base.hbs}}